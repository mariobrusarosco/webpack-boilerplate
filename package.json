{
  "name": "webpack-boilerplate",
  "version": "4.1.0",
  "description": "Webpack Boilerplate",
  "private": true,
  "configuration": {
    "css_modules": false,
    "sass": false,
    "scss": true,
    "stylus": false,
    "postcss": false
  },
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack/webpack.dev.config.js",
    "prod": "webpack --config ./webpack/webpack.prod.config.js",
    "server": "yarn prod && serve --no-clipboard --single --listen 8080 dist",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "e2e:dev-webpack": "start-server-and-test dev http-get://localhost:8080 cy:open",
    "e2e:dev": "start-server-and-test  server :8080 cy:open",
    "e2e:prod": "start-server-and-test  server :8080 cy:run"
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/webpack-boilerplate#readme",
  "dependencies": {
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "cypress": "^3.1.3",
    "live-serve": "^0.0.0",
    "serve": "^10.1.1",
    "start-server-and-test": "^1.7.11",
    "webpack-dev-server": "^3.1.10"
  }
}
