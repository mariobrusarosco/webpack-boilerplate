{
  "name": "front-end-boilerplate",
  "version": "4.8.0",
  "description": "Front End Boilerplate",
  "private": true,
  "scripts": {
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "unit-test:watch": "yarn eslint && yarn stylelint && jest --watchAll",
    "unit-test": "yarn eslint && yarn stylelint && jest",
    "e2e-dev": "start-server-and-test serve-front-end :8080 cy-open",
    "e2e-prod": "start-server-and-test serve-front-end :8080 cy-run",
    "cy-run": "cypress run",
    "cy-open": "cypress open",
    "server": "yarn prod-front && node server/index.js",
    "prod-front": "export NODE_ENV=production && webpack --config ./webpack/configuration/production.config.js",
    "serve-front-end": "yarn prod-front && node server/serve-front-end.js",
    "dev-front": "webpack-dev-server -r dotenv/config --config ./webpack/configuration/development.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn e2e-prod"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": [
        "eslint"
      ],
      "*.{js,css,scss,yml}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/webpack-boilerplate#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "axios": "^0.19.0",
    "babel-jest": "^24.9.0",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "ramda": "^0.26.1",
    "rc-pagination": "^1.20.5",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "cypress": "^3.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "favicons-webpack-plugin": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-trace-loader": "^0.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "react-hot-loader": "^4.9.0",
    "react-slick": "^0.25.2",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
