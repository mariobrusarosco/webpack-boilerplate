{
  "name": "webpack-boilerplate",
  "version": "4.3.0",
  "description": "Webpack Boilerplate",
  "private": true,
  "scripts": {
    "dev-front": "webpack-dev-server -r dotenv/config --config ./webpack/configuration/development.config.js",
    "prod-front": "webpack --config ./webpack/configuration/production.config.js",
    "serve-front-end": "yarn prod-front && node server/serve-front-end.js",
    "dev-back": "nodemon -r dotenv/config server/index.js",
    "prod-back": "",
    "server": "yarn prod-front && node server/index.js",
    "e2e-dev-webpack": "start-server-and-test serve-front-end http-get://localhost:8080 cy-run",
    "e2e-dev": "start-server-and-test serve-front-end :8080 cy-open",
    "e2e-prod": "start-server-and-test serve-front-end :8080 cy-run",
    "cy-run": "cypress run",
    "cy-open": "cypress open",
    "lint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn e2e-prod"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,css}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/webpack-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "cypress": "^3.2.0",
    "dotenv": "^6.2.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
